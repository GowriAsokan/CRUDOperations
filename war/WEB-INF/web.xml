http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>CRUDOpeartions</servlet-name>
		<servlet-class>com.sample.myproject.CRUDOpeartionsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CRUDOpeartions</servlet-name>
		<url-pattern>/crudopeartions</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>RetrieveServlet</servlet-name>
		<servlet-class>com.sample.myproject.RetrieveServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RetrieveServlet</servlet-name>
		<url-pattern>/retrieve</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>DeleteServlet</servlet-name>
		<servlet-class>com.sample.myproject.DeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteServlet</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
